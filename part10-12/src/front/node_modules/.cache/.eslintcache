r.jsx",[],[],"/home/dungodan/AndroidStudioProjects/PART_8/backend/src/front/src/components/Home.jsx",["116"],[],"import React from \"react\";\n\nclass Home extends React.Component {\n  constructor(props) {\n    super(props);\n  }\n\n  render() {\n    return (\n      <div className=\"mt-5 me-auto\">\n        <h2>RPO Art Frontend</h2>\n      </div>\n    );\n  }\n}\n\nexport default Home;\n","/home/dungodan/AndroidStudioProjects/PART_8/backend/src/front/src/components/Login.jsx",["117","118"],[],"import React, { useState } from \"react\";\nimport BackendService from \"../services/BackendService\";\nimport Utils from \"../utils/Utils\";\nimport { useNavigate } from \"react-router-dom\";\nimport { connect, userDispatch } from \"react-redux\"; // not use where dispatch comes from\nimport { userActions } from \"../utils/Rdx\";\n\n//export default connect()(function Login() );\n\nconst Login = ({ dispatch }) => {\n  const [username, setUsername] = useState(\"\");\n  const [password, setPassword] = useState(\"\");\n  const [loggingIn, setLoggingIn] = useState(false);\n  const [submitted, setSubmitted] = useState(false);\n  //const [error_message, setErrorMessage] = useState(null);\n  const nav = useNavigate();\n\n  function handleChangeLogin(e) {\n    setUsername(e.target.value);\n  }\n\n  function handleChangePassword(e) {\n    setPassword(e.target.value);\n  }\n\n  function handleSubmit(e) {\n    e.preventDefault();\n    setSubmitted(true);\n    //setErrorMessage(null);\n    setLoggingIn(true);\n    BackendService.login(username, password)\n      .then((resp) => {\n        console.log(resp.data);\n        setLoggingIn(false);\n        dispatch(userActions.login(resp.data));\n        nav(\"/home\");\n      })\n      .catch((err) => {\n        if (err.response && err.response.status === 401) {\n        }\n        //setErrorMessage(\"Ошибка авторизации\");\n        //setErrorMessage(err.message);\n        else setLoggingIn(false);\n      });\n  }\n\n  return (\n    <div className=\"col-md-6 me-0\">\n      {/*error_message && (\n        <div className=\"alert alert-danger mt-1 me-0 ms-0\">{error_message}</div>\n      )*/}\n      <h2>Вход</h2>\n      <form name=\"form\" onSubmit={handleSubmit}>\n        <div className=\"form-group\">\n          <label htmlFor=\"username\">Логин</label>\n          <input\n            type=\"text\"\n            className={\n              \"form-control\" + (submitted && !username ? \" is-invalid\" : \"\")\n            }\n            name=\"username\"\n            value={username}\n            onChange={handleChangeLogin}\n          />\n          {submitted && !username && (\n            <div className=\"help-block text-danger\">\n              Введите имя пользователя\n            </div>\n          )}\n        </div>\n        <div className=\"form-group\">\n          <label htmlFor=\"password\">Пароль</label>\n          <input\n            type=\"password\"\n            className={\n              \"form-control\" + (submitted && !password ? \" is-invalid\" : \"\")\n            }\n            name=\"password\"\n            value={password}\n            onChange={handleChangePassword}\n          />\n          {submitted && !password && (\n            <div className=\"help-block text-danger\">Введите пароль</div>\n          )}\n        </div>\n        <div className=\"form-group mt-2\">\n          <button className=\"btn btn-primary\">\n            {loggingIn && (\n              <span\n                className=\"spinner-border spinner-border-sm\"\n                role=\"status\"\n                aria-hidden=\"true\"\n              ></span>\n            )}\n            Вход\n          </button>\n        </div>\n      </form>\n    </div>\n  );\n};\n\nexport default connect()(Login);\n","/home/dungodan/AndroidStudioProjects/PART_8/backend/src/front/src/services/BackendService.jsx",["119"],[],"/home/dungodan/AndroidStudioProjects/PART_8/backend/src/front/src/utils/Utils.jsx",["120","121"],[],"import React from \"react\";\n\nclass Utils {\n  saveUser(user) {\n    localStorage.setItem(\"user\", JSON.stringify(user));\n  }\n\n  removeUser() {\n    localStorage.removeItem(\"user\");\n  }\n\n  getToken() {\n    let user = JSON.parse(localStorage.getItem(\"user\"));\n    return user && \"Bearer \" + user.token;\n  }\n\n  getUserName() {\n    let user = JSON.parse(localStorage.getItem(\"user\"));\n    return user && user.login;\n  }\n\n  getUser() {\n    return JSON.parse(localStorage.getItem(\"user\"));\n  }\n}\n\nexport default new Utils();\n","/home/dungodan/AndroidStudioProjects/PART_8/backend/src/front/src/utils/Rdx.jsx",[],[],"/home/dungodan/AndroidStudioProjects/PART_8/backend/src/front/src/components/SideBar.jsx",[],[],"/home/dungodan/AndroidStudioProjects/PART_8/backend/src/front/src/components/Alert.jsx",[],[],"/home/dungodan/AndroidStudioProjects/PART_8/backend/src/front/src/components/PaginationComponent.jsx",["122"],[],"/home/dungodan/AndroidStudioProjects/PART_8/backend/src/front/src/components/country/CountryComponent.jsx",["123","124","125"],[],"/home/dungodan/AndroidStudioProjects/PART_8/backend/src/front/src/components/country/CountryListComponent.jsx",["126"],[],"/home/dungodan/AndroidStudioProjects/PART_8/backend/src/front/src/components/artist/ArtistComponent.jsx",["127","128","129"],[],"/home/dungodan/AndroidStudioProjects/PART_8/backend/src/front/src/components/artist/ArtistListComponent.jsx",["130","131"],[],"/home/dungodan/AndroidStudioProjects/PART_8/backend/src/front/src/components/painting/PaintingComponent.jsx",["132","133","134"],[],"/home/dungodan/AndroidStudioProjects/PART_8/backend/src/front/src/components/painting/PaintingListComponent.jsx",["135"],[],"/home/dungodan/AndroidStudioProjects/PART_8/backend/src/front/src/components/museum/MuseumListComponent.jsx",["136","137"],[],"/home/dungodan/AndroidStudioProjects/PART_8/backend/src/front/src/components/museum/MuseumComponent.jsx",["138","139","140"],[],"/home/dungodan/AndroidStudioProjects/PART_8/